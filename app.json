{
  "expo": {
    "name": "FTA-Unifarm-Project_Base",
    "slug": "FTA-Unifarm-Project_Base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/toppng.com-vegetables-images-png-vegetables-1911x1320.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/toppng.com-vegetables-images-png-vegetables-1911x1320.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "google.author.FTA"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "google.author.FTA"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
